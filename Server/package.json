{
  "name": "idahub",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-client": "cd ./src/views/web-client/; ./build.sh; cd ../../../; ",
    "compress-client": "tar -zcvf ./dist/public/static/client.tar.gz ../Client",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files && npm run 'compress-client'",
    "start": "npm run build && forever start --minUptime 1000 --spinSleepTime 1000 ./dist/bin/www",
    "stop": "forever stop ./dist/bin/www"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "escape-string-regexp": "^2.0.0",
    "express": "~4.16.0",
    "express-validator": "^5.3.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.3",
    "mongoose-paginate": "^5.0.3",
    "morgan": "~1.9.0",
    "password-hash": "^1.2.2",
    "react-cookie": "^4.0.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.13.0"
  }
}
